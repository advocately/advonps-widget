    "@glimmer/test-helpers": "^0.30.0",
    "@types/qunit": "^2.0.31",
    "broccoli-asset-rev": "^2.5.0",
    "color": "^2.0.0",
    "ember-cli": "^2.14.0",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-dependency-checker": "^2.0.1",
    "ember-cli-inject-live-reload": "^1.6.1",
