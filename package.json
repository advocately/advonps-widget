  "description": "In-app NPS form",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build -e production",
    "start": "ember server",
    "test": "ember test",
    "lint-eslint": "eslint --ext .js, src",
    "lint-stylelint": "stylelint src/ui/styles/*.scss --fix",
